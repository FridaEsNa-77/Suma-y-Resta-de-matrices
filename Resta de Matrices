#include<stdio.h>

int main(){
	
	int matriz_1[2][2]={{6,5},{5,6}};
	int matriz_2[2][2]={{10,11},{11,10}};
	int matriz_3[2][2];
	int i,j;
	
	for(i=0;i<2;i++){
		for(j=0;j<2;j++){
			matriz_3[i][j]= matriz_1[i][j]-matriz_2[i][j];
		}
	}
	
	for(i=0;i<2;i++){
		for(j=0;j<2;j++){
			printf("%d",matriz_3[i][j]);
		}
		printf("\n");
	}
	
	return 0;
}
